{    
    "version":"2",
    "builds":[
        {
        "src":"./index.js",
        "use":"@vercel/node"
        },
        {
            "src": "./Client/build",
            "use": "@vercel/static"
          }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"index.js"
        }
    ]
}